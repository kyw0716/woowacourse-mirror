{
  "rules": {
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "double"],
    "no-console": "off",
    "comma-dangle": "off",
    "operator-linebreak": "off",
    "no-restricted-globals": "off",
    "no-undef": "off"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["airbnb"]
}
